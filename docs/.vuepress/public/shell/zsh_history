  HELM_INSTALL_DIR="${HOME}/bin" /tmp/get_helm.sh --no-sudo\
  rm /tmp/get_helm.sh\
\
  sudo curl --silent -L --output /usr/local/bin/skaffold "https://storage.googleapis.com/skaffold/releases/latest/skaffold-linux-${ARCH}"\
  sudo chmod +x /usr/local/bin/skaffold\
  skaffold version\
\
  sudo wget --quiet -O /usr/local/bin/bazel "https://github.com/bazelbuild/bazelisk/releases/latest/download/bazelisk-linux-${ARCH}"\
  sudo chmod +x /usr/local/bin/bazel\
  bazel --version\
\
