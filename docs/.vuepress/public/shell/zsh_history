  echo 'kernel.sysrq=1' | sudo tee /etc/sysctl.d/99-kernel.conf\
  echo 'vm.swappiness=4' | sudo tee /etc/sysctl.d/99-vm.conf\
  # echo 'fs.inotify.max_user_watches=8192' | sudo tee /etc/sysctl.d/99-fs.conf\
