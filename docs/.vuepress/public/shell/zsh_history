: 1664285000:12;install_kubectl_tools() {\
  echo '-------------------------------------------------------------------------------'\
  mkdir -pv ~/go/bin ~/go/.bin ~/bin ~/.bin\
  sudo mkdir -p /usr/local/bin/\
  ARCH="$([ $(uname -m) = "aarch64" ] && echo "arm64" || echo "amd64")"\
\
  curl -fsSL --output /tmp/get_helm.sh "https://raw.githubusercontent.com/helm/helm/main/scripts/get-helm-3"\
  chmod +x /tmp/get_helm.sh\
  HELM_INSTALL_DIR="${HOME}/bin" /tmp/get_helm.sh --no-sudo\
  rm /tmp/get_helm.sh\
\
  curl -L --output /usr/local/bin/skaffold "https://storage.googleapis.com/skaffold/releases/latest/skaffold-linux-${ARCH}"\
  sudo chmod +x /usr/local/bin/skaffold\
  skaffold version\
\
  sudo wget -O /usr/local/bin/bazel "https://github.com/bazelbuild/bazelisk/releases/latest/download/bazelisk-linux-${ARCH}"\
  sudo chmod +x /usr/local/bin/bazel\
\
  curl -L --output /usr/local/bin/minikube "https://storage.googleapis.com/minikube/releases/latest/minikube-linux-${ARCH}"\
  sudo chmod +x /usr/local/bin/minikube\
  minikube version\
\
  CURRENT_VERSION="$(curl -s https://checkpoint-api.hashicorp.com/v1/check/terraform | jq -r -M '.current_version')/terraform_$(curl -s https://checkpoint-api.hashicorp.com/v1/check/terraform | jq -r -M '.current_version')"\
  curl --output /tmp/terraform.zip "$(echo "https://releases.hashicorp.com/terraform/${CURRENT_VERSION}_linux_${ARCH}.zip")"\
  sudo unzip /tmp/terraform.zip -d /usr/local/bin\
  chmod +x /usr/local/bin/terraform\
  terraform --version || terraform version\
  rm /tmp/terraform.zip\
\
  curl -LO "https://dl.k8s.io/release/$(curl -L -s https://dl.k8s.io/release/stable.txt)/bin/linux/${ARCH}/kubectl"\
  sudo install -o root -g root -m 0755 kubectl /usr/local/bin/kubectl\
  rm kubectl\
  kubectl version\
\
  go install sigs.k8s.io/kind@latest\
  echo '-------------------------------------------------------------------------------'\
}\
