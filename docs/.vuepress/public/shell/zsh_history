EOF\
 cat << EOF | sudo tee -a /etc/modprobe.d/iwlwifi.conf\
# https://wireless.wiki.kernel.org/en/users/drivers/iwlwifi\
options iwlwifi power_save=n\
options iwlwifi power_level=5\
#options iwlwifi debug=0x43fff\
EOF\
 cat << EOF | sudo tee -a /etc/modprobe.d/cfg80211.conf\
# https://nullr0ute.com/2021/03/setting-the-wireless-regulatory-domain/\
# https://wireless.wiki.kernel.org/en/developers/regulatory/crda\
# https://www.linuxquestions.org/questions/slackware-14/network-manager-wifi-regional-settings-4175559295/\
options cfg80211 ieee80211_regdom=GB\
EOF\
 sudo apt install -y iw\
 sudo iw reg set GB\
 \
\
 \
 \
 cat << EOF | sudo tee /etc/modprobe.d/usbcore.conf\
options usbcore autosuspend=-1\
EOF\
 cat << EOF | sudo tee /etc/modprobe.d/i915.conf\
options i915 mitigations=off\
options i915 guc_log_level=4\
options i915 enable_guc=2\
options i915 fastboot=1\
options i915 reset=1\
options i915 enable_fbc=0\
options i915 enable_dc=0\
options i915 modeset=1\
EOF\
 cat << EOF | sudo tee /etc/modprobe.d/drm.conf\
# Enable all\
options drm debug=0x3f\
# options drm debug=0x19F\
# if want to trace DRM_DEBUG_KMS and DRM_DEBUG_DRIVER log\
# options drm debug=0x6\
EOF\
 cat << EOF | sudo tee /etc/modprobe.d/nvidia.conf\
options nvidia debug=0x19F\
options nvidia NVreg_EnableGpuFirmwareLogs=7\
options nvidia NVreg_OpenRmEnableUnsupportedGpus=1\
\
blacklist nouveau\
options nouveau modeset=0\
EOF\
}\
